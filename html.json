			"\t\t\t<div class=\"carousel-caption d-none d-md-block\">",
			"\t\t\t\t<h5>...</h5>",
			"\t\t\t\t<p>...</p>",
			"\t\t\t</div>",
			"\t\t</div>",
			"\t</div>",
			"\t<a class=\"carousel-control-prev\" href=\"#carouselExampleIndicators\" role=\"button\" data-slide=\"prev\">",
