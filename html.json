			"\t\t\t<div class=\"carousel-caption d-none d-md-block\">",
			"\t\t\t\t<h5>...</h5>",
			"\t\t\t\t<p>...</p>",
			"\t\t\t</div>",
			"\t\t</div>",
			"\t\t<div class=\"carousel-item\">",
			"\t\t\t<img class=\"d-block w-100\" src=\"ctrlfandreplacethisforimageurl\" alt=\"Second slide\">",
			"\t\t\t<div class=\"carousel-caption d-none d-md-block\">",
			"\t\t\t\t<h5>...</h5>",
			"\t\t\t\t<p>...</p>",
			"\t\t\t</div>",
			"\t\t</div>",
			"\t\t<div class=\"carousel-item\">",
